  agent any
  
  stages {
    stage('Build') {
      steps {
          echo 'This is Build' 
      }
    }
  }
  
  stages {
    stage('Test') {
      steps {
          echo 'This is Test' 
      }
    }
  }
  
  stages {
    stage('Deploy') {
      steps {
          echo 'This is Deploy' 
      }
    }
  }
  
}
